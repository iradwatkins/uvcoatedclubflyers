  "requests": [
    {
      "id": "1",
      "colId": "uv-coated-flyers-tests",
      "folderId": "shipping",
      "name": "Calculate FedEx Shipping Rates (Atlanta)",
      "method": "POST",
      "url": "{{baseUrl}}/api/shipping/calculate",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"toAddress\": {\n    \"street\": \"976 Carr Street\",\n    \"city\": \"Atlanta\",\n    \"state\": \"GA\",\n    \"zipCode\": \"30318\",\n    \"country\": \"US\",\n    \"isResidential\": false\n  },\n  \"items\": [\n    {\n      \"quantity\": 5000,\n      \"weightLbs\": 40.0\n    }\n  ]\n}",
        "form": []
      },
      "tests": [
        {
          "type": "res-code",
          "value": "200"
        },
        {
          "type": "json-query",
          "value": "$.success",
          "expected": "true"
        },
        {
          "type": "json-query",
          "value": "$.rates[?(@.carrier=='FEDEX' && @.service=='FEDEX_GROUND')]",
          "expected": "exists"
        }
      ]
    },
    {
      "id": "2",
      "colId": "uv-coated-flyers-tests",
