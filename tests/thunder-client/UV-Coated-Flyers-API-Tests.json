      ]
    },
    {
      "id": "3",
      "colId": "uv-coated-flyers-tests",
      "folderId": "shipping",
      "name": "Get Southwest Cargo Airports in Georgia",
      "method": "GET",
      "url": "{{baseUrl}}/api/shipping/airports?state=GA",
      "headers": [],
      "tests": [
        {
          "type": "res-code",
          "value": "200"
        },
        {
          "type": "json-query",
          "value": "$[?(@.code=='ATL')]",
          "expected": "exists"
        },
        {
          "type": "json-query",
          "value": "$[?(@.code=='ATL')].name",
          "expected": "contains:Hartsfield"
        }
      ]
    },
