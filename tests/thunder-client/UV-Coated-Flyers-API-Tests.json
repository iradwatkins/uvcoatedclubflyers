      "body": {
        "type": "json",
        "raw": "{\n  \"toAddress\": {\n    \"street\": \"6000 North Terminal Parkway\",\n    \"city\": \"Atlanta\",\n    \"state\": \"GA\",\n    \"zipCode\": \"30320\",\n    \"country\": \"US\",\n    \"isResidential\": false\n  },\n  \"items\": [\n    {\n      \"quantity\": 5000,\n      \"weightLbs\": 40.0\n    }\n  ],\n  \"selectedAirportId\": \"ATL\"\n}",
        "form": []
      },
      "tests": [
        {
          "type": "res-code",
          "value": "200"
        },
        {
          "type": "json-query",
          "value": "$.success",
          "expected": "true"
        },
        {
